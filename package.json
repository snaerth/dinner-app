{
  "name": "dinner-app",
  "description": "dinner application that helps you plan and organize your dinner meals",
  "version": "0.0.1",
  "scripts": {
    "format": "npx prettier --write \"{,!(node_modules)/**/}src/**/*.{js,svelte,json}\"",
    "eslint": "eslint --fix --quiet \"{,!(node_modules)/**/}src/**/*.{js,svelte}\"",
    "stylelint": "stylelint \"./**/*.{scss,svelte}\" --fix --quiet",
    "precommit": "concurrently \"npm:format\" \"npm:stylelint\" \"npm:eslint\" \"npm:test\"",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bulma-checkradio": "^1.1.1",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "session-file-store": "^1.3.1",
    "sirv": "^0.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/svelte": "^1.11.0",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^24.9.0",
    "bulma": "^0.8.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-svg-import": "^1.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "sapper": "^0.27.9",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "svelte": "^3.16.5",
    "svelte-jester": "^1.0.3",
    "svelte-preprocess": "^3.3.0"
  }
}